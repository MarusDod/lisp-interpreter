(setf a 0)
(loop
    (setf a (+ a 1))
    (if (and (eq (mod a 3) 0) (eq (mod a 5) 0)) 
        (print "fizzbuzz")
        (if (eq (mod a 3) 0) 
            (print "fizz")
            (if (eq (mod a 5) 0)
                (print "buzz")
                (print a)
            )
        )
    )
    (if (eq a 50) abort)
)
